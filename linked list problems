#include<stdio.h>
#include<stdlib.h>
struct node
{
    int data;
    struct node *link;
};
void traverse(struct node *ptr)
{
    if(ptr==NULL)
    {
        printf("empty linked list\n");
    }
    else{
        printf("elements is ");
        while(ptr != NULL)
        {
            printf("%d ",ptr->data);
            ptr=ptr->link;
        }
    }
}
struct node* at_first(struct node *ptr,int data)
{
    struct node *temp=(struct node *)malloc(sizeof(struct node));
    temp->data=data;
    temp->link=ptr;
    return temp;
}
struct node* at_end(struct node *ptr,int data)
{
    struct node *temp=(struct node *)malloc(sizeof(struct node));
    temp->data=data;
    temp->link=NULL;
    if (ptr == NULL) 
    {
        return temp;
    }
    struct node *p=ptr;
    while(ptr->link != NULL)
    {
        ptr=ptr->link;
    }
    ptr->link=temp;
    return p;
}
int main()
{
    int size,i,data;
    struct node *head=NULL;
    printf("enter node size ");
    scanf("%d",&size);
    printf("enter elements of linked list ");
    for(i=0;i<size;i++)
    {
        scanf("%d",&data);
        head=at_end(head,data);
    }
    traverse(head);
    printf("\nafter adding node at first\n");
    head=at_first(head,99);
    traverse(head);
    return 0;
}
